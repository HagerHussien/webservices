Name: news.addUser
Binding: newsserviceBinding
Endpoint: http://localhost/webservices/wsdl/server.php
SoapAction: http://localhost/webservices/wsdl/server.php/news.addUser
Style: rpc
Input:
  use: encoded
  namespace: 
  encodingStyle: http://schemas.xmlsoap.org/soap/encoding/
  message: news.addUserRequest
  parts:
    name: xsd:string
    email: xsd:string
    pass: xsd:string
Output:
  use: encoded
  namespace: 
  encodingStyle: http://schemas.xmlsoap.org/soap/encoding/
  message: news.addUserResponse
  parts:
    return: xsd:string
Namespace: 
Transport: http://schemas.xmlsoap.org/soap/http
